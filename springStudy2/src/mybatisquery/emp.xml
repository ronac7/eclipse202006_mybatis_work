<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
          "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
          
<mapper namespace="a.b.c.com.emp.dao.EmpDAO">

<select id="empSelectAll" resultType="empvo">
	SELECT
			A.EMPNO			EMPNO,
			A.ENAME			ENAME,
			A.HIREDATE		HIREDATE
	FROM	EMP A
</select>

</mapper>